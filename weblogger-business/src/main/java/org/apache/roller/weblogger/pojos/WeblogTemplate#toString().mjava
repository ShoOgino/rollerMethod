    public String toString() {
        StringBuffer buf = new StringBuffer();
        buf.append("{");
        buf.append(this.id);
        buf.append(", ").append(this.name);
        buf.append(", ").append(this.link);
        buf.append(", ").append(this.decoratorName);
        buf.append(", ").append(this.templateLanguage);
        buf.append("}");
        return buf.toString();
    }

