    @Override
    public String toString() {
        StringBuffer buf = new StringBuffer();
        buf.append("{");
        buf.append(getId());
        buf.append(", ").append(getName());
        buf.append(", ").append(getTimeAquired());
        buf.append(", ").append(getTimeLeased());
        buf.append("}");
        return buf.toString();
    }

