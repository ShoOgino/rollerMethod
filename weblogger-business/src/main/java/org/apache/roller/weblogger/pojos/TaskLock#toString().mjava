    @Override
    public String toString() {
        StringBuffer buf = new StringBuffer();
        buf.append("{");
        buf.append(this.id);
        buf.append(", ").append(this.name);
        buf.append(", ").append(this.timeAquired);
        buf.append(", ").append(this.timeLeased);
        buf.append("}");
        return buf.toString();
    }

