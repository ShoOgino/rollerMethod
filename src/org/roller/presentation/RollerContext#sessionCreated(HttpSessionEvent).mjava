    public void sessionCreated(HttpSessionEvent se)
    {
        if (mMemDebug)
        {
            mSessionCount.increment();
            mContext.log(
                "Roller:SESSION_CREATED:count="
                    + mSessionCount
                    + ":freemem="
                    + Runtime.getRuntime().freeMemory()
                    + ":totmem="
                    + Runtime.getRuntime().totalMemory());
        }
    }

