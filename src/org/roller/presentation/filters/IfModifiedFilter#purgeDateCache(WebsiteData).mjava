    public static void purgeDateCache(WebsiteData website) 
    {
        String handle = (website != null) ? website.getHandle() : null;
        StringBuffer sb = new StringBuffer();
        sb.append("zzz_");
        sb.append(handle);
        sb.append("_zzz_");
        getCache().purge(new String[] {sb.toString()});
    }

