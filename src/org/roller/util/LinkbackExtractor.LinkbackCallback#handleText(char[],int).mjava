        /**
         * Get the page title 
         */
        public void handleText(char[] data, int pos)
        {
            if ( mCurrentTag != null && mCurrentTag.equals(Tag.TITLE) )
            {
                String newText = new String(data);
                if (mTitle.length() < 50)
                {
                    mTitle += newText;
                }
            }
        }

