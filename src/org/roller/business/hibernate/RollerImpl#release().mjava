	public void release()
	{
		if (mUserMgr != null)
			mUserMgr.release();
		if (mWeblogMgr != null)
			mWeblogMgr.release();
		if (mRefererMgr != null)
			mRefererMgr.release();
		try
		{
			if (mStrategy != null)
				mStrategy.release(); 
		}
		catch (Exception e)
		{
			mLogger.error(
			"Exception with mSupport.release() [" + e + "]", e);
		}
	}

