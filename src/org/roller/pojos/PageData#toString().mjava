   public String toString()
   {
      StringBuffer str = new StringBuffer("{");

      str.append("id=" + id + " " + "name=" + name + " " + "description=" 
      + description + " " + "link=" + link + " " + "template=" + template 
      + " " + "updateTime=" + updateTime);
      str.append('}');

      return(str.toString());
   }

