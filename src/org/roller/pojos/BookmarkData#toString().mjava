    public String toString()
    {
        StringBuffer str = new StringBuffer("{");

        str.append("id=" + id + " " + "name=" + name + " " + "description=" + 
                   description + " " + "url=" + url + " " + "weight=" + 
                   weight + " " + "priority=" + priority + " " + "folderId=" + 
                   "image=" + image + " " + "feedUrl=" + 
                   feedUrl);
        str.append('}');

        return (str.toString());
    }

