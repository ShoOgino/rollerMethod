    /**
     * Get a print writer
     *
     * @throws IOException
     */
    public PrintWriter getWriter() throws IOException {
       if (cachedWriter == null) {
           cachedWriter = new SplitPrintWriter(
              new PrintWriter(new OutputStreamWriter(result.getOutputStream(),"UTF-8")), 
              super.getWriter());
       }
       return cachedWriter;
    }

