    public static Date parseFromFormats(String aValue) {        if (StringUtils.isEmpty(aValue)) return null;                // get DateUtil's formats        SimpleDateFormat formats[] = DateUtil.getFormats();        if (formats == null) return null;                // iterate over the array and parse        Date myDate = null;        for (int i = 0; i <formats.length; i++) {            try {                myDate = DateUtil.parse(aValue, formats[i]);                //if (myDate instanceof Date)                return myDate;            } catch (Exception e) {                // do nothing because we want to try the next                // format if current one fails            }        }        // haven't returned so couldn't parse        return null;    }
