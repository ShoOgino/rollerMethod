    /**     * Tries to take the passed-in String and format it as a date string in the     * the passed-in format.     **/    public static String formatDateString(String aString, SimpleDateFormat aFormat) {        if (StringUtils.isEmpty(aString) || aFormat == null)  return "";        try {            java.sql.Timestamp aDate = parseTimestampFromFormats(aString);            if (aDate != null) {                return DateUtil.format(aDate, aFormat);            }        } catch (Exception e) {            // Could not parse aString.        }        return "";    }
