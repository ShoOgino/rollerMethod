    /**
     * Instantiates and returns an instance of HibernatePlanetImpl.
     */
    public static Planet instantiate() throws RollerException {
        if (me == null) {
            log.debug("Instantiating HibernateRollerPlanetImpl");
            me = new HibernateRollerPlanetImpl();
        }
        
        return me;
    }

