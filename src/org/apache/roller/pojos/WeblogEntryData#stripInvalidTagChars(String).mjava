     public String stripInvalidTagChars(String s) 
     {
       StringBuffer sb = new StringBuffer();
       char [] charArray = s.toCharArray();
       for(int i = 0; i < charArray.length; i++) 
       {
         if(checkValidTagChar(charArray[i]))
           sb.append(charArray[i]);
       }
       return sb.toString();
     }

