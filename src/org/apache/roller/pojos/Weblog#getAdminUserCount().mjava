    public int getAdminUserCount() {
        int count = 0;
        WeblogPermission userPerms = null;
        Iterator iter = getPermissions().iterator();
        while (iter.hasNext()) {
            WeblogPermission perms = (WeblogPermission) iter.next();
            if (perms.getPermissionMask() == WeblogPermission.ADMIN) {
                count++;
            }
        }
        return count;
    }

