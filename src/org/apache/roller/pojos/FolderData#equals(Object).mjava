    public boolean equals(Object other) {
        
        if (other == null) return false;
        
        if (other instanceof FolderData) {
            FolderData o = (FolderData) other;
            return new EqualsBuilder()
                .append(getPath(), o.getPath()) 
                //.append(getWebsite(), o.getWebsite()) 
                .isEquals();
        }
        
        return false;
    }    

