    public String toString() {
        
        StringBuffer str = new StringBuffer("{");
        str.append("id=" + id + ", " +
                "name=" + name + ", " +
                "email=" + email + ", " +
                "url=" + url + ", " +
                "postTime=" + postTime + ", " +
                "status=" + status);
        str.append("}");
        
        return str.toString();
    }

