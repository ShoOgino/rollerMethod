    public String toString() {
        StringBuilder buf = new StringBuilder();
        buf.append("{");
        buf.append(getId());
        buf.append(", ").append(getHandle());
        buf.append(", ").append(getName());
        buf.append(", ").append(getEmailAddress());
        buf.append(", ").append(getLocale());
        buf.append(", ").append(getTimeZone());
        buf.append("}");
        return buf.toString();
    }

