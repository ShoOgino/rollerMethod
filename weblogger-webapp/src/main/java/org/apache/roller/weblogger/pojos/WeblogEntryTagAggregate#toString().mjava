    public String toString() {
        StringBuffer buf = new StringBuffer();
        buf.append("{");
        buf.append(this.id);
        buf.append(", ").append(this.name);
        buf.append(", ").append(this.total);
        buf.append(", ").append(this.lastUsed);
        buf.append("}");
        return buf.toString();
    }

