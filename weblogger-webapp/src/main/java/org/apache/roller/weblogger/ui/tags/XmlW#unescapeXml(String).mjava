    static public String unescapeXml(String str) {
        str = StringUtils.replace(str,"&amp;","&");
        str = StringUtils.replace(str,"&lt;","<");
        str = StringUtils.replace(str,"&gt;",">");
        str = StringUtils.replace(str,"&quot;","\"");
        str = StringUtils.replace(str,"&apos;","'");
        return str;
    }

