    public void testSelfRefererDenial() throws RollerException    {        // test against "self referrals"        //getRoller().begin();        // create "direct" referer        rmgr.processRequest(            new MockRequest(                "20020101",                "direct",                "http://test.com",                null,mWebsite            )        );        getRoller().commit();        int newRefCount = rmgr.getReferers(mWebsite).size();                // now create self-referer        //getRoller().begin();        rmgr.processRequest(            new MockRequest(                "20020202",                "http://test.com/page/" + mWebsite.getUser().getUserName(),                "http://test.com",                null,mWebsite            )        );        getRoller().commit();                // number of referrers should not have changed        refs = rmgr.getReferers(mWebsite);                    assertEquals("self referal not ignored", newRefCount, refs.size());     }
