    public void testRefererProcessing() throws RollerException {                RefererManager rmgr = RollerFactory.getRoller().getRefererManager();                List refs = rmgr.getReferers(testWeblog);        assertEquals("number of referers should equal count", count, refs.size());                int hits = rmgr.getDayHits(testWeblog);        assertEquals("There should be one fewer hits than referers", count, hits);    }
