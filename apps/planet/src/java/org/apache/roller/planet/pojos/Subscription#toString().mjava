    public String toString() {
        StringBuffer buf = new StringBuffer();
        
        buf.append("{");
        buf.append(feedUrl).append(", ");
        buf.append(siteUrl).append(", ");
        buf.append(title).append(", ");
        buf.append(author).append(", ");
        buf.append(lastUpdated);
        buf.append("}");
        
        return buf.toString();
        
    }

