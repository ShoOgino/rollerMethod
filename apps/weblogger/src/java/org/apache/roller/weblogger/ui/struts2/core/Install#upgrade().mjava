    public String upgrade() {
        
        if(WebloggerFactory.isBootstrapped()) {
            return SUCCESS;
        }
        
        try {
            messages = WebloggerStartup.upgradeDatabase(true);
            
            success = true;
        } catch (StartupException se) {
            error = true;
            messages = se.getStartupLog();
        }
        
        return UPGRADE_DATABASE;
    }

