    /**
     * Show admin user edit page.
     */
    public String execute() {
        
        if (getUser() != null && getUser().getId() != null) {
            // populate form data from user profile data
            getBean().copyFrom(getUser(), getLocale());
        } else {
            addError("userAdmin.error.userNotFound");
            return ERROR;
        }

        return INPUT;
    }

