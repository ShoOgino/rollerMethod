    public String toString() {
        StringBuffer buf = new StringBuffer();
        buf.append("{");
        buf.append(this.id);
        buf.append(", ").append(this.userName);
        buf.append(", ").append(this.attrName);
        buf.append(", ").append(this.attrValue);
        buf.append("}");
        return buf.toString();
    }

