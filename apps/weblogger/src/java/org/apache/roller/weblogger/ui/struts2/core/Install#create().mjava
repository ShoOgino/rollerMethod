    public String create() {
        
        if(RollerFactory.isBootstrapped()) {
            return SUCCESS;
        }
        
        try {
            messages = WebloggerStartup.createDatabase();
            
            success = true;
        } catch (StartupException se) {
            error = true;
            messages = se.getStartupLog();
        }
        
        return CREATE_DATABASE;
    }

